// Header Styling Empty File
#main-header 
	border-bottom 1px solid bsttre
	position relative
	
	.logo
		color ww
		font-weight 700
		font-size 1.44em
		line-height 67px
		margin 0 16px
	
	.top-head
		background bdark
		height 67px
		position relative
		
	.bottom-head
		padding 16px 0
		position relative
		text-align center

#main-nav
	position absolute
	right 0
	top 0
	
	ul 
		list-style none
		margin 12px 16px
		padding 0
		
	li 
		display inline-block
		vertical-align top
		margin-left 10px
		
	a:not(.std-btn)
		color ww
		line-height 42px
		
		&:hover 
			color bclear
			text-decoration underline